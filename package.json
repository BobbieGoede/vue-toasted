{
	"name": "vue-toasted",
	"description": "Responsive Touch Compatible Toast plugin for VueJS 2+",
	"version": "1.1.28",
	"author": "Shakeeb Sadikeen <shakee.zats@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/shakee93/vue-toasted.git"
	},
	"main": "./dist/vue-toasted.js",
	"types": "./src/types/index.d.ts",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack serve --open --hot",
		"build": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress",
		"build-local-watch": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress --watch",
		"release": "npm run build && npm run core && npm run css",
		"css": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.css.js --progress",
		"core": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.core.js --progress",
		"es": "babel src --presets babel-preset-es2015 --out-dir dist"
	},
	"keywords": [
		"toast",
		"vue",
		"vue-toasted",
		"toasted",
		"responsive",
		"touch-compatible",
		"touch",
		"vue-toast",
		"vue-toastr",
		"toastr"
	],
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.14.2",
		"@babel/preset-env": "^7.14.2",
		"@types/shortid": "0.0.29",
		"animejs": "^3.2.1",
		"autoprefixer": "^10.2.5",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.2.2",
		"babel-preset-es2015": "^6.22.0",
		"babel-preset-latest": "^6.24.1",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.4",
		"file-loader": "^6.2.0",
		"hammerjs": "^2.0.8",
		"mini-css-extract-plugin": "^1.6.0",
		"node-sass": "^6.0.0",
		"postcss-loader": "^5.3.0",
		"prettier": "^2.3.0",
		"requirejs": "^2.3.6",
		"sass-loader": "^11.1.1",
		"shortid": "^2.2.16",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.1.2",
		"ts-loader": "^9.1.2",
		"typescript": "^4.2.4",
		"vue": "^2.6.12",
		"vue-loader": "^15.9.7",
		"vue-style-loader": "^4.1.3",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.37.0",
		"webpack-bundle-analyzer": "^4.4.1",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2"
	}
}
