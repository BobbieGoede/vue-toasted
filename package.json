{
	"name": "vue-toasted",
	"description": "Responsive Touch Compatible Toast plugin for VueJS 2+",
	"version": "1.1.28",
	"author": "Shakeeb Sadikeen <shakee.zats@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/shakee93/vue-toasted.git"
	},
	"sideEffects": true,
	"main": "./dist/vue-toasted.js",
	"types": "./types/index.d.ts",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack serve --open",
		"build": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress",
		"build:analytics": "cross-env NODE_ENV=production ANALYTICS=true webpack --config ./build/webpack.release.js --progress",
		"build:css": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.css.js --progress",
		"build:core": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.core.js --progress",
		"release": "npm run build && npm run build:core && npm run build:css"
	},
	"keywords": [
		"toast",
		"vue",
		"vue-toasted",
		"toasted",
		"responsive",
		"touch-compatible",
		"touch",
		"vue-toast",
		"vue-toastr",
		"toastr"
	],
	"devDependencies": {
		"@babel/core": "^7.14.2",
		"@babel/preset-env": "^7.14.2",
		"@egjs/hammerjs": "^2.0.17",
		"@types/shortid": "0.0.29",
		"animejs": "^3.2.1",
		"autoprefixer": "^10.2.5",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.2.2",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.4",
		"fibers": "^5.0.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.3.1",
		"mini-css-extract-plugin": "^1.6.0",
		"postcss-loader": "^5.3.0",
		"prettier": "^2.3.0",
		"sass": "^1.32.13",
		"sass-loader": "^11.1.1",
		"shortid": "^2.2.16",
		"terser-webpack-plugin": "^5.1.2",
		"ts-loader": "^9.1.2",
		"typescript": "^4.2.4",
		"vue": "^2.6.12",
		"vue-loader": "^15.9.7",
		"vue-style-loader": "^4.1.3",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.37.0",
		"webpack-bundle-analyzer": "^4.4.1",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2"
	}
}
